Description:
sinf() function: sine function.

Files:
lib/sinf.c
m4/sinf.m4

Depends-on:
math
extensions
sin             [test $HAVE_SINF = 0]

configure.ac:
gl_FUNC_SINF
gl_CONDITIONAL([GL_COND_OBJ_SINF], [test $HAVE_SINF = 0])
gl_MATH_MODULE_INDICATOR([sinf])

Makefile.am:
if GL_COND_OBJ_SINF
lib_SOURCES += sinf.c
endif

Include:
<math.h>

Link:
$(SINF_LIBM)

License:
LGPL

Maintainer:
all
