Description:
tanf() function: tangent function.

Files:
lib/tanf.c
m4/tanf.m4

Depends-on:
math
extensions
tan             [test $HAVE_TANF = 0]

configure.ac:
gl_FUNC_TANF
gl_CONDITIONAL([GL_COND_OBJ_TANF], [test $HAVE_TANF = 0])
gl_MATH_MODULE_INDICATOR([tanf])

Makefile.am:
if GL_COND_OBJ_TANF
lib_SOURCES += tanf.c
endif

Include:
<math.h>

Link:
$(TANF_LIBM)

