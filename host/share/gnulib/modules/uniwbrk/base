Description:
Base layer for word breaks.

Files:
lib/uniwbrk.in.h
m4/libunistring-base.m4

Depends-on:
gen-header
unitypes

configure.ac:
gl_LIBUNISTRING_LIBHEADER([0.9.11], [uniwbrk.h])
AC_PROG_MKDIR_P

Makefile.am:
BUILT_SOURCES += $(LIBUNISTRING_UNIWBRK_H)

uniwbrk.h: uniwbrk.in.h
@NMD@	$(AM_V_GEN)$(MKDIR_P) '%reldir%'
	$(gl_V_at)$(SED_HEADER_TO_AT_t) $(srcdir)/uniwbrk.in.h
	$(AM_V_at)mv $@-t $@
MOSTLYCLEANFILES += uniwbrk.h uniwbrk.h-t

Include:
"uniwbrk.h"

License:
LGPLv3+ or GPLv2+

Maintainer:
all
