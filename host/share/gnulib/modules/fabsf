Description:
fabsf() function: absolute value.

Files:
lib/fabsf.c
m4/fabsf.m4
m4/mathfunc.m4

Depends-on:
math
extensions
fabs            [test $HAVE_FABSF = 0]

configure.ac:
gl_FUNC_FABSF
gl_CONDITIONAL([GL_COND_OBJ_FABSF], [test $HAVE_FABSF = 0])
gl_MATH_MODULE_INDICATOR([fabsf])

Makefile.am:
if GL_COND_OBJ_FABSF
lib_SOURCES += fabsf.c
endif

Include:
<math.h>

Link:
$(FABSF_LIBM)

License:
LGPL

Maintainer:
all
