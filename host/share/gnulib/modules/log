Description:
log() function: natural logarithmic function.

Files:
lib/log.c
m4/log.m4
m4/mathfunc.m4

Depends-on:
math

configure.ac:
AC_REQUIRE([gl_FUNC_LOG])
gl_CONDITIONAL([GL_COND_OBJ_LOG], [test $REPLACE_LOG = 1])
gl_MATH_MODULE_INDICATOR([log])

Makefile.am:
if GL_COND_OBJ_LOG
lib_SOURCES += log.c
endif

Include:
<math.h>

Link:
$(LOG_LIBM)

