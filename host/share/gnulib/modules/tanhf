Description:
tanhf() function: hyperbolic tangent function.

Files:
lib/tanhf.c
m4/tanhf.m4

Depends-on:
math
extensions
tanh            [test $HAVE_TANHF = 0]

configure.ac:
gl_FUNC_TANHF
gl_CONDITIONAL([GL_COND_OBJ_TANHF], [test $HAVE_TANHF = 0])
gl_MATH_MODULE_INDICATOR([tanhf])

Makefile.am:
if GL_COND_OBJ_TANHF
lib_SOURCES += tanhf.c
endif

Include:
<math.h>

Link:
$(TANHF_LIBM)

License:
LGPL

Maintainer:
all
