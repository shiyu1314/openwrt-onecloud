Description:
rint() function: round according to the current rounding mode.

Files:
lib/rint.c
m4/rint.m4
m4/mathfunc.m4

Depends-on:
math

configure.ac:
gl_FUNC_RINT
gl_CONDITIONAL([GL_COND_OBJ_RINT], [test $HAVE_RINT = 0])
gl_MATH_MODULE_INDICATOR([rint])

Makefile.am:
if GL_COND_OBJ_RINT
lib_SOURCES += rint.c
endif

Include:
<math.h>

Link:
$(RINT_LIBM)

License:
LGPL

Maintainer:
all
