Description:
floorf() function: round towards negative infinity.

Files:
lib/floorf.c
lib/floor.c
m4/floorf.m4

Depends-on:
math
extensions
float           [test $HAVE_DECL_FLOORF = 0 || test $REPLACE_FLOORF = 1]

configure.ac:
gl_FUNC_FLOORF
gl_CONDITIONAL([GL_COND_OBJ_FLOORF],
               [test $HAVE_DECL_FLOORF = 0 || test $REPLACE_FLOORF = 1])
gl_MATH_MODULE_INDICATOR([floorf])

Makefile.am:
if GL_COND_OBJ_FLOORF
lib_SOURCES += floorf.c
endif

Include:
<math.h>

Link:
$(FLOORF_LIBM)

License:
LGPL

Maintainer:
all
