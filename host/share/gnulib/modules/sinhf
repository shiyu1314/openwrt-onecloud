Description:
sinhf() function: hyperbolic sine function.

Files:
lib/sinhf.c
m4/sinhf.m4

Depends-on:
math
extensions
sinh            [test $HAVE_SINHF = 0]

configure.ac:
gl_FUNC_SINHF
gl_CONDITIONAL([GL_COND_OBJ_SINHF], [test $HAVE_SINHF = 0])
gl_MATH_MODULE_INDICATOR([sinhf])

Makefile.am:
if GL_COND_OBJ_SINHF
lib_SOURCES += sinhf.c
endif

Include:
<math.h>

Link:
$(SINHF_LIBM)

License:
LGPL

Maintainer:
all
