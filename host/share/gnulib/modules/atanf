Description:
atanf() function: inverse tangent function.

Files:
lib/atanf.c
m4/atanf.m4

Depends-on:
math
extensions
atan            [test $HAVE_ATANF = 0]

configure.ac:
gl_FUNC_ATANF
gl_CONDITIONAL([GL_COND_OBJ_ATANF], [test $HAVE_ATANF = 0])
gl_MATH_MODULE_INDICATOR([atanf])

Makefile.am:
if GL_COND_OBJ_ATANF
lib_SOURCES += atanf.c
endif

Include:
<math.h>

Link:
$(ATANF_LIBM)

License:
LGPL

Maintainer:
all
