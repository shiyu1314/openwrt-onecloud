Description:
acosf() function: inverse cosine function.

Files:
lib/acosf.c
m4/acosf.m4

Depends-on:
math
extensions
acos            [test $HAVE_ACOSF = 0]

configure.ac:
gl_FUNC_ACOSF
gl_CONDITIONAL([GL_COND_OBJ_ACOSF], [test $HAVE_ACOSF = 0])
gl_MATH_MODULE_INDICATOR([acosf])

Makefile.am:
if GL_COND_OBJ_ACOSF
lib_SOURCES += acosf.c
endif

Include:
<math.h>

Link:
$(ACOSF_LIBM)

License:
LGPL

Maintainer:
all
