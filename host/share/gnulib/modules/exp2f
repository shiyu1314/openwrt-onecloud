Description:
exp2f() function: exponential base 2 function.

Files:
lib/exp2f.c
m4/exp2f.m4

Depends-on:
math
extensions
exp2            [test $HAVE_EXP2F = 0]

configure.ac:
gl_FUNC_EXP2F
gl_CONDITIONAL([GL_COND_OBJ_EXP2F], [test $HAVE_EXP2F = 0])
gl_MATH_MODULE_INDICATOR([exp2f])

Makefile.am:
if GL_COND_OBJ_EXP2F
lib_SOURCES += exp2f.c
endif

Include:
<math.h>

Link:
$(EXP2F_LIBM)

