Description:
sqrtf() function: square root.

Files:
lib/sqrtf.c
m4/sqrtf.m4

Depends-on:
math
extensions
sqrt            [test $HAVE_SQRTF = 0]

configure.ac:
gl_FUNC_SQRTF
gl_CONDITIONAL([GL_COND_OBJ_SQRTF], [test $HAVE_SQRTF = 0])
gl_MATH_MODULE_INDICATOR([sqrtf])

Makefile.am:
if GL_COND_OBJ_SQRTF
lib_SOURCES += sqrtf.c
endif

Include:
<math.h>

Link:
$(SQRTF_LIBM)

License:
LGPL

Maintainer:
all
